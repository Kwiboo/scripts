CONFIG_PREBOOT="env set autoload no;env set boot_targets pxe;"
CONFIG_USE_PREBOOT=y
CONFIG_BOOTCOMMAND="bootflow scan -lbG"
CONFIG_BOOTP_PREFER_SERVERIP=y
CONFIG_BOOTSTD_FULL=y
